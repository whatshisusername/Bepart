{
  "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/frontend/build/**/*",
        "use": "@vercel/static"
      }
    ],
  
    "routes": [
      {
               "src": "/api/(.*)",
                 "dest": "src/index.js"
               },
        
      
      {
        "src": "/(.*)",
        "dest": "src/frontend/build/index.html"
      }
    ]
  }