{
  "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/frontend/build/**/*",
        "use": "@vercel/static"
      }
    ],
  
    "routes": [
      {
               "src": "/(.*)",
                 "dest": "src/index.js"
               }
        
      
      
    ],
    "functions": {
    "api/*.js": {
      "maxDuration": 60
    }
  }

  }